{
  "name": "@acme/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:deploy": "yarn with-env prisma migrate deploy",
    "db:generate": "yarn with-env prisma generate",
    "db:push": "yarn with-env prisma db push --skip-generate",
    "db:seed": "yarn with-env prisma db seed",
    "dev": "yarn with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@acme/shared": "^0.1.0",
    "@prisma/client": "^5.0.0",
    "lodash": "^4.17.21",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "dotenv-cli": "^7.2.1",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
